/*运行监控通用样式*/
ul,li{list-style-type: none;}
.mr_tp20{margin-top: 20px;}
.mr_bt20{margin-bottom:20px;}
.clear_border{border:none;}
.tr{text-align:right}
.tf{text-align:left;}
/*头部统计模块*/
.time-head{width: 23.8%;margin:20px 0;height:95px;position: absolute;top: 0px;left: 0px;font-family: "微软雅黑";color: #299FE4;display: none}
.time-head .day{font-size: 22px;margin: 0px;position: absolute;left: 0px;top: 0px;}
.time-head .time{font-size: 65px;margin: 21px 0px 0px 0px;}
@media (max-width: 1320px) {
    .time-head .day{font-size: 18px;font-weight: bolder;}
    .time-head .time{font-size: 60px;margin: 27px 0px 0px 0px;}
}
@media (max-width: 1200px) {
    .time-head .day{font-size: 22px}
    .time-head .time{font-size: 47px;margin: 39px 0px 0px 0px;}
}
@media (max-width: 1024px) {
    .time-head .day{font-size: 16px}
    .time-head .time{font-size: 47px;margin: 43px 0px 0px 0px;}
}
.monitor-head{width:74.2%;height:95px;margin:20px 0;}
.monitor-head ul li{float:left;width:23.5%;border:1px solid #ddd;height:95px;margin-left:2%;}
.monitor-head ul li:FIRST-CHILD{margin-left:0%;}
.monitor-head ul li>div{width:100%;padding:0 10px;}
.mask{background-color: #2b2f3b;height: 95px;float: left;position: absolute;top: 0px;left: 0px;opacity: 0.1;filter: alpha(opacity=0.1);}
.wrap-head span{display: inline-block;height:68px;line-height:68px;width:100%;text-align:right;color:#2b2f3b;font-family: Helvetica,Arial;font-size:30px;}
.wh-fr0 span{background: url("../images/time_monitor.png") no-repeat left center;}
.wh-fr1 span{background: url("../images/time_all.png") no-repeat left center;}
.wh-fr2 span{background: url("../images/time_normal.png") no-repeat left center;}
.wh-fr3 span{background: url("../images/time_error.png") no-repeat left center;}
.wh-fr4 span{background: url("../images/time_disable.png") no-repeat left center;}
.wrap-bottom{height:27px;line-height:27px;}
.wb-fr0{background: #755ad8}
.wb-fr1{background: #3c8dbc}
.wb-fr2{background: #009966}
.wb-fr3{background: #e53417}
.wb-fr4{background: #ffa012}
.wb-text1{color: #3c8dbc}
.wb-text2{color: #009966}
.wb-text3{color: #e53417}
.wrap-bottom span{display: inline-block;float:left;color:#fff;}


.panel-default>.panel-heading{border-left:5px solid #3c8dbc}

.row-table{width:100%;border-collapse: collapse;color:#9ca2a6;}
.row-table tr th{line-height:60px;height:60px;text-align:center;font-weight:100;}
.row-table tr td{text-align:center;padding:20px 0;}
.row-table tbody tr{border-bottom:1px solid #ddd;height:40px;}
.row-table tr .stu-th{text-align:left;}
.row-table tr .stu-td{text-align:left;}
.stu-td p:FIRST-CHILD{font-size:16px;color:#3c8dbc;}
.stu-span{padding-left:25px;display: inline-block;font-weight:600;}
.stu-normal{background: url("../images/icon_normal.png") no-repeat left center;color:#009966;}
.stu-erp{margin:0;cursor: pointer;width:100%;text-align: center;}
.stu-error{background: url("../images/icon_error.png") no-repeat left center;color:#e53417;cursor: pointer;}
.stu-disable{background: url("../images/icon_disable.png") no-repeat left center;color:#ffa012}


/*区域管理页面样式*/
.area-content{position: relative;margin-bottom: 0px}
.area-table{margin-left: 350px;padding-left: 20px}
ul.nav-tabs-underline li.active a:focus{border: 0px;background-color: white;}
.area-tree{margin-top: -1px;width:23.8%;position: absolute;top: 0px;left: 0px;height: 100%;}
.area-tree .panel-body{padding-left: 14px;margin-top: 4px;}
.area-tree .title{font-size: 15px}
.area-tree .panel-heading{background-color: #f5f5f4; border: 0px;border-left:5px solid #3c8dbc}
.area-tree .panel-default{border-radius: 2px; border-color: white; box-shadow: none;margin: 0px;}
.area-tree .btns{margin-right: -10px;}
.area-tree .font-btn{margin-right: 10px;cursor: pointer;}
.area-tree .update{font-size: 16px}

.area-tree .ztree{overflow-y:scroll;padding-bottom:30px;}
/*树的配置*/
.ztree li span.button.switch.level0 {visibility:hidden; width:1px;}
.ztree li ul.level0 {padding:0; background:none;}

/*页面显示数量的样式*/
.show_tip{height:40px;line-height: 40px;font-size:18px;letter-spacing: 1px;}
.show_tip span{margin:0 5px;font-weight:600;}
.totalCount{color:#0099CC}
.normalCount{color:#009966}
.errorCount{color:#FF3300}
.disableCount{color:#FFA012}


/*修改异常状态弹出框*/
.bootstrap-dialog-footer{float: right;}
.btn-model-diy{width: 60px;}
.statusMonitorConfig-dialog .modal-dialog{width: 480px;height: 400px;min-height: 400px}

.error-list{position: absolute;left:25.8%;width:74.2%;}
.panel-nobody .endpoints{overflow-y:auto;padding-bottom:30px;}
.error-content ul{padding:10px 0px;margin:0;padding-left:15px;}
.error-content li{margin-bottom:15px;padding-left: 0;}
.error_status{position: relative;}
.selected-info,.error_status .btn-group{position: absolute;top:-6px;right:15px;}
.selected-info{right: 225px;top: 0px;}
.selected-info strong{font-size: 16px}
.rows-content{width:100%;background: #ebf3ff;border:1px solid #b0ceff;padding-bottom: 5px;padding-top:15px;}
.rows-title{font-size:16px;color:#3671cf;text-align: center;font-weight:600}
.rows-count{text-align: center;padding-top:13px;overflow: hidden;}
.rows-count p{margin:0;line-height:25px;}
.rows-count div{padding: 0}
.rows-normal{color:#36cf73;font-size:16px;font-family: Helvetica,Arial;}
.rows-normal:LAST-CHILD{font-weight:600;}
.rows-error{font-family: Helvetica,Arial;color:#cf3636;font-size:16px;box-sizing: border-box;cursor: pointer;}
.rows-error:LAST-CHILD{}
.rows-disable{font-family: Helvetica,Arial;color:#525854;font-size:16px;border-left:1px solid #90b2e7;box-sizing: border-box;}
.rows-foot{text-align: center;font-size:12px;color:#7f7f7f;margin-top: 10px;}

#inform{position: absolute;top: 20px;width: 350px;max-height: 250px;z-index: 10;background-color: #E0E5E5; overflow: auto;display: none;}
.block-message{display:none;position: fixed;background: white;z-index: 100;}
.block-content{border: 1px solid #ccc; padding: 10px 15px 15px 15px;-moz-box-shadow: 0px 0px 20px #666;box-shadow: 0px 0px 20px #666;-webkit-box-shadow: 0px 0px 20px #666;height:300px;min-width:590px;overflow-y:auto; }
.block-content table{margin: 0px}
.block-content table .detail > td{padding: 0px !important;}
.block-content table .detail{display: none;}
.block-content table .detail-content{padding: 5px 8px}
.block-content .table-nobody td{border: 0px;border-bottom: 0px !important;padding: 4px 4px 0px 0px !important;}

/*异常信息查询detail-view*/
.detail-icon i{color:#737272;}
.table-hover>tbody>tr.detail-view:hover{background-color: inherit}
.detail-view td{border: 0px !important;}
.detail-view .table{border-bottom: 0px}
.detail-view .table{padding: 4px;}
.warning-info{margin: 0px 10px 0px 0px;  position: absolute;bottom: 38px;right: 19px;}
.warning-info .warning{color: #f0ad4e;margin-right: 10px}